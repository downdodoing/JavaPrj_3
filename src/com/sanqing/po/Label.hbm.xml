<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping 
			PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Label" table="tb_label"><!-- 每个class对应一个持久化对象 -->
		<id name="labelID"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<generator class="identity"></generator><!--指定主键生成策略 -->
		</id>
		<property name="labelRightID"></property><!-- 映射subjectID属性 -->
		<property name="labelName"></property><!-- 映射labelName属性 -->
	</class>
</hibernate-mapping>